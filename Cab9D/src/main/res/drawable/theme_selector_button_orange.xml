<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_window_focused="false" android:state_enabled="true"
	      android:drawable="@drawable/theme_button_orange_normal" />
	<item android:state_window_focused="false" android:state_enabled="false"
	      android:drawable="@drawable/theme_button_orange_disabled_focused" />
	<item android:state_pressed="true"
	      android:drawable="@drawable/theme_button_orange_pressed" />
	<item android:state_focused="true" android:state_enabled="true"
	      android:drawable="@drawable/theme_button_orange_focused" />
	<item android:state_enabled="true"
	      android:drawable="@drawable/theme_button_orange_normal" />
	<item android:state_focused="true"
	      android:drawable="@drawable/theme_button_orange_focused" />
	<item
		android:drawable="@drawable/theme_button_orange_disabled" />
</selector>